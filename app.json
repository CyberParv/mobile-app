{
  "expo": {
    "name": "Expo Production Scaffold",
    "slug": "expo-production-scaffold",
    "scheme": "exposcaffold",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0B1220"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
